package com.pojo;

public class Goods1Pojo {
	private int id;
	private String name;
	private int idDelete;
	public Goods1Pojo(){}
	public Goods1Pojo(int id, String name, int idDelete) {
		super();
		this.id = id;
		this.name = name;
		this.idDelete = idDelete;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getIdDelete() {
		return idDelete;
	}
	public void setIdDelete(int idDelete) {
		this.idDelete = idDelete;
	}
}
