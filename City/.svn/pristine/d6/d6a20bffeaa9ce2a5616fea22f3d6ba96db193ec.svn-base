package com.pojo;

import java.util.Date;
// 陈旭超订单表
public class CityOrder {
	
	private int id;// id
	private String data;// 订单时间
	private String orderNumber;// 订单号
	private int quantity;// 商品数量
	private double totalprice;// 商品总价格
	private int typeid;// 订单状态id
	private int userid;// 用户id
	private int storeid;// 店铺id
	private int shopsid;// 店铺id
	private int detailsid;// 商品id
	private String address;//详细地址
	private String consignee;//收货人
	private String phone;//手机号
	private String pay;//支付方式
	private String distribution;//快递方式
	private String message;//用户留言
	
	
	private CityOrdertype ordertype;// 订单状态实体类
	private CityStore store;// 店铺实体类
	private shopsPojo shops;// 店铺实体类
	private shop_details details;// 商品实体类
	
	private String str;
	
	
	public int getShopsid() {
		return shopsid;
	}
	public void setShopsid(int shopsid) {
		this.shopsid = shopsid;
	}
	public shopsPojo getShops() {
		return shops;
	}
	public void setShops(shopsPojo shops) {
		this.shops = shops;
	}
	public String getStr() {
		return str;
	}
	public void setStr(String str) {
		this.str = str;
	}
	public CityOrdertype getOrdertype() {
		return ordertype;
	}
	public void setOrdertype(CityOrdertype ordertype) {
		this.ordertype = ordertype;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public String getConsignee() {
		return consignee;
	}
	public void setConsignee(String consignee) {
		this.consignee = consignee;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getPay() {
		return pay;
	}
	public void setPay(String pay) {
		this.pay = pay;
	}
	public String getDistribution() {
		return distribution;
	}
	public void setDistribution(String distribution) {
		this.distribution = distribution;
	}
	public String getMessage() {
		return message;
	}
	public void setMessage(String message) {
		this.message = message;
	}
	public CityStore getStore() {
		return store;
	}
	public void setStore(CityStore store) {
		this.store = store;
	}
	public shop_details getDetails() {
		return details;
	}
	public void setDetails(shop_details details) {
		this.details = details;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getData() {
		return data;
	}
	public void setData(String data) {
		this.data = data;
	}
	public String getOrderNumber() {
		return orderNumber;
	}
	public void setOrderNumber(String orderNumber) {
		this.orderNumber = orderNumber;
	}
	public int getQuantity() {
		return quantity;
	}
	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}
	public double getTotalprice() {
		return totalprice;
	}
	public void setTotalprice(double totalprice) {
		this.totalprice = totalprice;
	}
	public int getTypeid() {
		return typeid;
	}
	public void setTypeid(int typeid) {
		this.typeid = typeid;
	}
	public int getUserid() {
		return userid;
	}
	public void setUserid(int userid) {
		this.userid = userid;
	}
	public int getStoreid() {
		return storeid;
	}
	public void setStoreid(int storeid) {
		this.storeid = storeid;
	}

	public int getDetailsid() {
		return detailsid;
	}
	public void setDetailsid(int detailsid) {
		this.detailsid = detailsid;
	}
	public CityOrder(){}
	public CityOrder(int id, String data, String orderNumber, int quantity,
			double totalprice, int typeid, int userid, int storeid) {
		this.id = id;
		this.data = data;
		this.orderNumber = orderNumber;
		this.quantity = quantity;
		this.totalprice = totalprice;
		this.typeid = typeid;
		this.userid = userid;
		this.storeid = storeid;
	}
	
	
	
}
